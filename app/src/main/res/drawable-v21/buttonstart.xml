<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape
        android:shape="oval" >
        <gradient android:startColor="@color/colorPrimary" android:endColor="@color/colorPrimary"  android:angle="45"   ></gradient>
    </shape>
</item>
<item android:state_pressed="true">
    <shape
        android:shape="oval" >
        <gradient android:startColor="@color/colorPressed" android:endColor="@color/colorPrimary"  android:angle="45"   ></gradient>
    </shape>
</item>
</selector>